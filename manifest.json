{
  "name": "GPT-3 Web3 Tweet Writer",
  "description": "Write a Twitter thread about any topic in Web3!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate tweet thread"
  },
  "permissions": ["contextMenus", "tabs", "storage"]
}
